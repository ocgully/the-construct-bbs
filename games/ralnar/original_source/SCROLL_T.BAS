DECLARE SUB UpdatePic ()
COMMON SHARED WorldX, WorldY
SCREEN 13
WorldX = 25
WorldY = 25
'MAX Screen(162, 220)
'Max Icon(203,69)
CLS
'$DYNAMIC

DIM SHARED Icons(203, 2)
DIM SHARED TestS(50, 50)

FOR T = 1 TO 2
   FOR X = 1 TO 20
     FOR Y = 1 TO 20
       PSET (X, Y), T
     NEXT Y
   NEXT X
   GET (1, 1)-(20, 20), Icons(1, T)
NEXT T
T = 0
FOR Y = 1 TO 50
  FOR X = 1 TO 50
    READ TestS(X, Y)
    'IF Y MOD 10 = 0 THEN RESTORE
  NEXT X
NEXT Y
    
'DONE INITIALIZING

CALL UpdatePic
DO
A$ = INKEY$
SELECT CASE A$
  
       CASE IS = CHR$(0) + "H"    'UP
         IF WorldY - 6 > 0 THEN
           WorldY = WorldY - 1
           CALL UpdatePic
         END IF

      CASE IS = CHR$(0) + "P"    'DOWN
         IF WorldY + 6 < 50 THEN
           WorldY = WorldY + 1
           CALL UpdatePic
         END IF
     
      CASE IS = CHR$(0) + "K"    'LEFT
         IF WorldX - 9 > 0 THEN
           WorldX = WorldX - 1
           CALL UpdatePic
         END IF
     
      CASE IS = CHR$(0) + "M"    'RIGHT
         IF WorldX + 9 < 50 THEN
           WorldX = WorldX + 1
           CALL UpdatePic
         END IF
END SELECT

LOCATE 2, 26: PRINT "X: "; WorldX; " Y: "; WorldY
LOOP UNTIL UCASE$(A$) = "Q"

END

